{
  "name": "becky-api-server",
  "version": "1.0.0",
  "description": "Becky API Server with Express, TypeScript, and Prisma",
  "main": "dist/index.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
